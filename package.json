{
  "name": "grp",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://gpuregistry.org",
  "dependencies": {
    "@openzeppelin/contracts": "^4.8.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.4",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.1",
    "react-scripts": "5.0.1",
    "styled-components": "^6.0.0-rc.1",
    "typescript": "^5.0.4",
    "web-vitals": "^2.1.4"
